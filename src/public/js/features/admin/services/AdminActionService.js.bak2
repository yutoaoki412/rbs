    // æ—¥æœ¬èªã®å€¤ã‚’è‹±èªã‚­ãƒ¼ã«ãƒãƒƒãƒ”ãƒ³ã‚°
    const globalStatus = this.#mapJapaneseStatusToKey(globalStatusRaw);
