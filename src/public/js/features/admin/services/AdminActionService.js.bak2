        if (sessionInfoContainer) {
          // 2æ™‚é–“æœªæº€ã®å ´åˆã¯è­¦å‘Šè¡¨ç¤º
          if (remainingMs < 2 * 60 * 60 * 1000) {
            sessionInfoContainer.classList.add('warning');
