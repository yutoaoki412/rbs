    };
    document.addEventListener('keydown', handleEscape);
    
    // ãƒ¢ãƒ¼ãƒ€ãƒ«èƒŒæ™¯ã‚¯ãƒªãƒƒã‚¯ã§é–‰ã˜ã‚‹
    modal.addEventListener('click', (e) => {
