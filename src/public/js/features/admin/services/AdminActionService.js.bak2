      this.error('ã‚»ãƒƒã‚·ãƒ§ãƒ³æƒ…å ±è¡¨ç¤ºæ›´æ–°ã‚¨ãƒ©ãƒ¼:', error);
      const sessionInfoElement = document.getElementById('session-remaining');
